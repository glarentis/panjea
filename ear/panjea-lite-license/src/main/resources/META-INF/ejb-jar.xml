<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	version="3.0">
	<assembly-descriptor>
    <interceptor-binding>
      <ejb-name>Panjea.MagazzinoDocumentoService</ejb-name>
      <interceptor-class>it.eurotn.panjea.lite.license.interceptors.DocumentiMagazzinoInterceptor</interceptor-class>
      <method>
        <method-name>salvaAreaMagazzino</method-name>
      </method>
    </interceptor-binding>
    <interceptor-binding>
      <ejb-name>Panjea.OrdiniDocumentoService</ejb-name>
      <interceptor-class>it.eurotn.panjea.lite.license.interceptors.DocumentiOrdiniInterceptor</interceptor-class>
      <method>
        <method-name>salvaAreaOrdine</method-name>
      </method>
    </interceptor-binding>
    <interceptor-binding>
      <ejb-name>Panjea.PreventiviDocumentoService</ejb-name>
      <interceptor-class>it.eurotn.panjea.lite.license.interceptors.DocumentiPreventiviInterceptor</interceptor-class>
      <method>
        <method-name>salvaAreaPreventivo</method-name>
      </method>
    </interceptor-binding>
    <interceptor-binding>
      <ejb-name>Panjea.AnagraficaService</ejb-name>
      <interceptor-class>it.eurotn.panjea.lite.license.interceptors.EntitaInterceptor</interceptor-class>
      <method>
        <method-name>salvaEntita</method-name>
      </method>
    </interceptor-binding>
    <interceptor-binding>
      <ejb-name>Panjea.ListinoService</ejb-name>
      <interceptor-class>it.eurotn.panjea.lite.license.interceptors.ListiniInterceptor</interceptor-class>
      <method>
        <method-name>salvaListino</method-name>
      </method>
    </interceptor-binding>
	</assembly-descriptor>
</ejb-jar>
