<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false">

	<diskStore path="java.io.tmpdir" />

	<cacheManagerEventListenerFactory
		class="" properties="" />

	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="true"
		diskSpoolBufferSizeMB="30" maxElementsOnDisk="10000000"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="panjea.datiGeografici" maxElementsInMemory="1000"
		eternal="true" overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />

	<cache name="panjea.preference" maxElementsInMemory="100"
		eternal="true" overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />



	<cache name="panjea.azienda" maxElementsInMemory="3" eternal="true"
		overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />

	<cache name="panjea.codiceIva" maxElementsInMemory="50" eternal="true"
		overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />

	<cache name="panjea.cap" maxElementsInMemory="1500" eternal="true"
		overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />

	<cache name="panjea.localita" maxElementsInMemory="1500" eternal="true"
		overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />

	<cache name="panjea.depositi" maxElementsInMemory="50" eternal="true"
		overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />

	<cache name="panjea.tipoDocumento" maxElementsInMemory="300"
