	<cache name="panjea.contabilitaSettings" maxElementsInMemory="10"
		eternal="true" overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />

	<cache name="panjea.tipoAreaContabile" maxElementsInMemory="100"
		eternal="true" overflowToDisk="true" memoryStoreEvictionPolicy="FIFO" />

	<cache name="panjea.registroIva" maxElementsInMemory="15"
